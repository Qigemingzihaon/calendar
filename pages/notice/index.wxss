.content {
    width: 100%;
    height: 100%;
    display: flex;
    -webkit-flex-direction: column;
    flex-direction: column;
    -webkit-align-items: center;
    align-items: center;
    -webkit-justify-content: flex-start;
    justify-content: flex-start;
    background-image: url('https://leaguer-1256376813.cos.ap-chengdu.myqcloud.com/67ff783675b84d7dbb06dad100a6a257.jpg');
    background-size: 100%;
    background-repeat: no-repeat;
    background-attachment: fixed;
    background-position: center;

}
.content .logo {
    width: 100%;
    /* margin-top: 34rpx; */
    /* padding: 0 0 0 29rpx; */
    height: 64rpx;
    margin-top: 10rpx;
}
.content .logo image {
    margin-left: 29rpx;
    width: 174rpx;
    height: 64rpx;
}
.content .title {
    width: 100%;
    height: 834rpx;
    margin-top: 10rpx;
}
.content .content_top{
    margin-top: 100rpx;
}
.content .title image {
    width: 100%;
    height: 100%;
}
.content .text {
    width: 100%;
    height: 31rpx;
    position: relative;
    margin-top: 10rpx;
    margin-bottom: 10rpx;
}
.content .text image {
    position: absolute;
    top: 0;
    width: 100%;
    height: 31rpx;
}
.content .btn {
    width: 100%;
    height: 153rpx;
    margin-top: 79rpx;
}
.content .btn image {
    width: 100%;
    height: 100%;
}
