.content{
    width: 100%;
    height: 100%;
    position: relative;
    display: flex;
    flex-direction:column;
    align-items: center;
    justify-content: flex-start;
    background-image: url('https://leaguer-1256376813.cos.ap-chengdu.myqcloud.com/67ff783675b84d7dbb06dad100a6a257.jpg');
    background-size: 100%;
    background-repeat: no-repeat;
    background-attachment: fixed;
    background-position: center;
}
.content .title{
    width: 100%;
    height: 715rpx;
    margin-top: 10rpx;
    position: relative;
}
